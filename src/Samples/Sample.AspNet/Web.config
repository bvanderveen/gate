<?xml version="1.0"?>
<configuration>
  
  <appSettings>
    <!-- when no middleware is used, pointing the host handler at the framework adapter is sufficient -->
    <!--<add key="Gate.Startup" value="Gate.Nancy.Application.Create"/>-->
    <add key="Gate.Startup" value="Sample.AspNet.Startup"/>
  </appSettings>
  
	<system.web>
    <!-- debug true to support 'F5' running -->
		<compilation debug="true" targetFramework="4.0" />
	</system.web>

  <system.webServer>
    <!-- plug the aspnet host handler's module -->
		<modules runAllManagedModulesForAllRequests="true">
			<add name="Gate" type="Gate.AspNet.Module, Gate.AspNet" />
		</modules>
	</system.webServer>
</configuration>